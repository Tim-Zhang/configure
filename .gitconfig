[user]
	name = TimZhang
	email = timzhang@dnspod.com
[alias]
   co = checkout
   ca = commit -a
   cm = commit -m
   st = status
   br = branch
   df = diff
   onelog = log -p -1
[diff]
   tool = vimdiff
[difftool]
   prompt = False
[core]
	editor = /usr/bin/vim
[merge]
	tool = kdiff3
[mergetool "kdiff3"]
	path = /Applications/kdiff3.app/Contents/MacOS/kdiff3
[push]
	default = upstream
